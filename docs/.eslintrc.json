{
    "env": {
        "browser": true,
        "es2021": true
    },
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:css/recommended"
	],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
		"css"
    ],
    "rules": {
		"react/react-in-jsx-scope": "off"
    }
}
